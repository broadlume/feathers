{
  "name": "@adhawk/analytics-pixel-loader",
  "description": "A webpack loader which builds Segment analytics.js pixels as small, efficient bundles",
  "version": "1.0.0",
  "author": "developers@tryadhawk.com",
  "license": "MIT",
  "typings": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "@adhawk/feathers"
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rm -Rf ./lib/* ./meta/bundlesize/* ./meta/coverage/* ./.rpt2_cache",
    "prepare": "npm run build",
    "prebuild": "npm run clean",
    "build": "tsc -p .",
    "watch": "tsc-watch --onSuccess \"npm run postbuild\" -p .",
    "predeploy": "npm run build",
    "deploy": "npm publish",
    "test": "jest --config ../../config/jest.config.js --testPathPattern packages/analytics-pixel-loader",
    "test:watch": "jest --config ../../config/jest.config.js --testPathPattern packages/analytics-pixel-loader --watch",
    "test:cjs": "npm run build && jest --config ../../config/jest.cjs.config.js --testPathPattern packages/analytics-pixel-loader"
  },
  "dependencies": {
    "@adhawk/error-tracking": "^1.1.6",
    "@segment/analytics.js-core": "^3.10.1",
    "@types/js-yaml": "^3.12.1",
    "js-yaml": "^3.13.1",
    "loader-utils": "^1.2.3",
    "lodash.kebabcase": "^4.1.1",
    "schema-utils": "^2.6.2",
    "tslib": "^1.10.0"
  },
  "main": "./lib/index.js",
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/json-schema": "^7.0.4",
    "@types/loader-utils": "^1.1.3",
    "@types/lodash.kebabcase": "^4.1.6",
    "jest": "24.9.0",
    "prettier": "^1.19.1",
    "tsc-watch": "4.0.0",
    "typescript": "3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "gitHead": "21d4253fee28bfb703c7ca25122f230a86669d3f"
}
